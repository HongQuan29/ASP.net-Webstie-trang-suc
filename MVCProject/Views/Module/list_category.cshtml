@model IEnumerable<MVCProject.Models.Category>
@{
    Layout = null;
}

<div class=" collapse d-md-flex border  " id="sidebar">
    <ul class="nav flex-column flex-nowrap w-100 bg-white list-group ">
        <li class="list-group-item bg-info text-white "> Danh Mục Sản Phẩm</li>

        @foreach (var item in Model)
        {
            @Html.Action("SubListCategory", "Module", new { parentid = item.ID })
        }
    </ul>
</div>
